<Window x:Class="PerfumeApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PerfumeApp"
        mc:Ignorable="d"
        Title="MainWindow" Icon="/Images/Восьмерка.ico" MaxHeight="500" MaxWidth="800" MinHeight="500" MinWidth="800">
    <StackPanel>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Image Source="/Images/Восьмерка.png" HorizontalAlignment="Center" Width="20" Height="20"/>
        </StackPanel>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" Width="800" Height="480"/>

    </StackPanel>
</Window>
